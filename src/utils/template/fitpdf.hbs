<html>

<head>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            background: #fff;
        }

        @page {
            size: 1500px 920px;
            margin: 3px;
        }


        body {
            font-family: "Poppins", sans-serif;
            background-color: #f9f9f9;
            background-color: var(--bg);
            color: #444;
            color: var(--black_text);
        }

        html {
            -webkit-text-size-adjust: 100%;
        }

        :root {
            --white: #fff;
            --bg: #f9f9f9;
            --green_base: #10b981;
            --bgProcess: #c9c9c9;
            --orange_base: #fb923c;
            --gray: #e5e5e5;
            --gray_text: #c8d6e5;
            --black_text: #444;
            --red_base: #ef4444;
            --blue_base: #0ea5e9;
            --gradient-green: linear-gradient(360deg, var(--green), #466451 116%);
            --radius: 0.625rem;
            --circuleRadius: 50%;
            --shadow: 0px 0px 1rem hsla(0, 0%, 82.4%, 0.65098);
            --paddingInput: .5rem;
            --negativeSpace: 2vw;
            --bold: 600;
            --regular: 400;
            --border_print: 2px solid var(--black_text);
        }

        .table-content {
            height: 3.5rem;
            max-height: 4rem;
        }

        .table-content {
            font-size: .7rem;
            border-radius: 3px;
            border: 1px solid #e4e4e4;
            width: 100%;
            text-align: center;
        }

        .table-content tr {
            height: auto;
        }

        .table-content tr td {
            font-size: .6rem;
            border-top: 1px solid var(--gray);
            padding: 4px;
        }

        .container-printPage .row-header {
            width: 100%;
            padding: 10px;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .container-printPage .row-secondary {
            width: 100%;
            display: grid;
            grid-template-columns: 1fr 1fr;
            grid-gap: 5px;
            gap: 5px;
        }

        .content-info {
            border: 1px solid #e4e4e4;
            border-radius: 4px;
            width: 100%;
            display: flex;
            justify-content: space-between;
            align-content: center;
            grid-gap: 1rem;
            gap: 1rem;
        }

        .inputModalHeader {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr;
            grid-gap: 5px;
            gap: 8px;
            font-size: .65rem;
        }

        .container-printPage .row-third,
        .container-printPage .row-third .second-row-third {
            display: grid;
            grid-gap: 5px;
            gap: 5px;
            grid-template-columns: 1fr 1fr;
        }

        .container-printPage .row-third {
            margin-top: 5px;
        }

        .first-row-third {
            display: grid;
            grid-template: -webkit-min-content 1fr min-content/1fr;
            grid-template: min-content 1fr min-content/1fr;
        }



        .internal-second-row-third {
            display: grid;
            grid-template: -webkit-min-content 1fr/1fr;
            grid-template: min-content 1fr/1fr;
        }

        .content {
            display: flex;
            flex-direction: column;
            grid-gap: 5px;
            gap: 5px;
            font-size: .7rem;
        }

        img {
            border-style: none;
        }

        .internal-second-row-third {
            display: grid;
            grid-template: -webkit-min-content 1fr/1fr;
            grid-template: min-content 1fr/1fr;
            grid-template-rows: min-content 1fr;
            grid-template-columns: 1fr;
            grid-template-areas: none;
        }

        .internal-second-row-third {
            display: grid;
            grid-template: -webkit-min-content 1fr/1fr;
            grid-template: min-content 1fr/1fr;
        }

        .table-content {
            height: 3rem;
            max-height: 4rem;
        }

        .info {
            border-radius: 3px;
            border: 1px solid #e4e4e4;
            padding: 5px;
            width: 100%;
            height: 2.5rem;
            text-align: left;
        }

        .info .info-header {
            display: flex;
            justify-content: space-between;
        }

        .content-info-fu {
            font-size: .7rem;
            border: 2px solid #e4e4e4;
            padding: 5px;
            border-radius: 5px;
        }

        .content-info-fu .content-tool {
            display: flex;
            justify-content: space-between;
        }

        .cardsFiles {
            margin-top: 5px;
            font-size: .7rem;
            border: 1px solid #e4e4e4;
            border-radius: 3px;
            padding: 5px;
        }

        .cardsFiles .header-card {
            display: flex;
            align-items: center;
            flex-wrap: wrap;
        }

        .cardsFiles .cards-items {
            grid-gap: 5px;
            gap: 5px;
            height: auto;
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr;
        }
    </style>
</head>

